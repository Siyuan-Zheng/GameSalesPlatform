<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="486.0" prefWidth="350.0" styleClass="root" stylesheets="@../resources/Theme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.shigure.controller.LoginController">
   <children>
      <StackPane fx:id="stackPane" prefHeight="486.0" prefWidth="350.0" styleClass="root">
         <children>
            <Pane fx:id="pane" prefHeight="472.0" prefWidth="350.0" styleClass="root">
               <children>
                  <JFXButton fx:id="butt_register" buttonType="RAISED" layoutX="70.0" layoutY="415.0" onAction="#click_register" prefHeight="27.0" prefWidth="67.0" styleClass="login-button" text="注册" textFill="#bfbfbf">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="butt_login" buttonType="RAISED" defaultButton="true" layoutX="205.0" layoutY="415.0" onAction="#click_login" prefHeight="27.0" prefWidth="67.0" styleClass="login-button" text="登录" textFill="#bfbfbf">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <effect>
                        <Reflection bottomOpacity="0.16" fraction="0.69" topOpacity="0.4" />
                     </effect>
                  </JFXButton>
                  <JFXTextField fx:id="field_userName" focusColor="#e8e8e8" labelFloat="true" layoutX="94.0" layoutY="174.0" prefHeight="27.0" prefWidth="191.0" promptText="UserName" unFocusColor="#f28c8c" />
                  <JFXPasswordField fx:id="field_password" focusColor="#ffff8d" labelFloat="true" layoutX="94.0" layoutY="264.0" prefHeight="27.0" prefWidth="191.0" promptText="Password" unFocusColor="#7c3c3c" />
                  <ImageView fx:id="img_lock" fitHeight="60.0" fitWidth="67.0" layoutX="145.0" layoutY="45.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../material/home.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="img_user" fitHeight="34.0" fitWidth="37.0" layoutX="53.0" layoutY="173.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../material/user.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="img_password" fitHeight="34.0" fitWidth="34.0" layoutX="53.0" layoutY="263.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../material/lock.png" />
                     </image>
                  </ImageView>
                  <JFXToggleButton fx:id="check_manager" layoutX="51.0" layoutY="327.0" prefHeight="55.0" prefWidth="106.0" size="8.0" text="管理员" />
               </children>
            </Pane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
